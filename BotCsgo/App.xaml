<Application x:Class="BotCsgo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BotCsgo"
             xmlns:converter="clr-namespace:BotCsgo.Converter"
             xmlns:helper="clr-namespace:BotCsgo.Helpers"
             StartupUri="MainWindow.xaml">

    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Fonts.xaml"/>
                <ResourceDictionary Source="Resources/Styles/TextBox.xaml"/>

                <ResourceDictionary Source="Resources/Styles/Button.xaml"/>
                <ResourceDictionary Source="Resources/Styles/Color.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <helper:ViewModelLocator x:Key="ViewModelLocator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
